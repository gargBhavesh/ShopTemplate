    function validateNewPassword() { 
        var newPassword = $("#NewPassword").val();
        var conPassword = $("#ConfirmPassword").val();
        var currPassword = $("#CurrentPassword").val();
        if(newPassword==""){
            $("#newPasswordFeedback").html("New Password can not be blank");
            return false;
        }
        else{
            if (newPassword.length >= 8) {
                 $("#passwordHighlight").children().addClass('active');
                 
            }
            else { 
                $("#passwordHighlight").children().removeClass('active');
                $("#newPasswordFeedback").html("New Password Length can not be less than 8");
                 return false;
            }
            if (newPassword == currPassword){
                $("#newPasswordFeedback").html("New Password can not be Same as Current Password");
                return false;
            }
            else {
                $("#newPasswordFeedback").html("");
                if (conPassword != "") {
                    var checkConandNewMatchFlag = checkBothPasswordsMatch();
                    return checkConandNewMatchFlag;
                     
                }
                else{
                     
                    return true;
                }
            }
        } 
