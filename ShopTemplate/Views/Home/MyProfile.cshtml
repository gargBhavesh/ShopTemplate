@inject Microsoft.AspNetCore.Http.IHttpContextAccessor _HttpContextAccessor
@{
    ViewData["Title"] = "User Profile | Shopivana";
    Layout = "~/Views/Shared/_Layout.cshtml";


    string username = "",profilePic="",emailId="";
    string EmailVerificationStatus = "Unverified";
    if (TempData.ContainsKey("UserName"))
    {
        if (!(TempData["UserName"] is null)) {
            username = TempData["UserName"].ToString();
            profilePic = TempData["ProfilePic"].ToString();
            emailId = TempData["EmailId"].ToString();
        }
        if(TempData["EmailVerificationStatus"] != null){
            if(TempData["EmailVerificationStatus"].ToString() == "1"){
                EmailVerificationStatus = "Verified";
            }
        }
    }
    if (profilePic == "")
    {
        profilePic = "blank.png";
    }
    String hostName = string.Empty,scheme = string.Empty;
    hostName = _HttpContextAccessor.HttpContext.Request.Host.ToString();
    scheme = _HttpContextAccessor.HttpContext.Request.Scheme.ToString();
    string profileSource = scheme + "://" + hostName + "/Images/ProfilePic/" + profilePic;
    
     
}

