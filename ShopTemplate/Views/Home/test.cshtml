        function SetupGuideLoad() {
            $.ajax({
                    url: '/Account/GetSetupGuide',
                    type: 'POST',
                    data: {},
                    success: function (returnData) {
                        if (returnData.Result == "OK") {
                            var sections = JSON.parse(returnData.sections);
                            var steps = JSON.parse(returnData.steps);
                            $("#SetupGuide").empty();
                            var strHtml = "";
                            for (var i = 0; i < sections.length; i++) {
                                var full = sections[i];
                                strHtml += `
                            <div class="accordion-item">
                                <h2 class="accordion-header" id="SetupGuide_header_`+ i + `">
                                    <button class="accordion-button fs-6 fw-bold `+ (i > 0 ? 'collapsed' : '') + `" type="button" data-bs-toggle="collapse" data-bs-target="#SetupGuide_body_` + i + `" aria-expanded="true" aria-controls="kt_accordion_1_body_1">
                                       `+ full.Section + `
                                    </button>
                                </h2>
                                <div id="SetupGuide_body_`+ i + `" class="accordion-collapse collapse ` + (i == 0 ? 'show' : '') + `" aria-labelledby="SetupGuide_header_` + i + `" data-bs-parent="#SetupGuide">
                                    <div class="accordion-body p-0 px-1">`;

                                var step = steps.filter(x => x.Section == full.Section);
                                for (var j = 0; j < step.length; j++) {

