        var IsShowSetup = ('True' === 'True');
        
        if (IsShowSetup && SetupSteps.length > 0) {
            ShowSetupStep();
        }
        function ShowSetupStep() {
            $(".setup-step").text("Step " + SetupSteps[0].ID + " of 7");
            $(".setup-svg").html(SetupSteps[0].Icon);
            $(".setup-title").text(SetupSteps[0].Title);
            $(".setup-desc").text(SetupSteps[0].Description);
            $(".setup-btn").attr('href', SetupSteps[0].URL);
            $(".setup-dismiss-btn").off('click');
            $(".setup-dismiss-btn").click(function () {
                SetupDismiss(SetupSteps[0].ID, 'setup-row', false);
            });
            $(".setup-row").slideDown(200);
        }
        $(document).ready(function () {
            getStats();
        });
        function showOnboarding() {
            $('.onboarding-card').removeClass('d-none');
            //$('.onboarding-card').addClass('animate__zoomIn animate__animated');
            $('#kt_post').addClass('d-none');
            $('#kt_post2').addClass('d-none');
        }
        function getStats() {
            $.ajax({
                url: "/Dashboard/GetStats",
                type: "POST",
                data: { },
                success: function (returnData) {
                    if (returnData.Result == "OK") {

                        var Progress = JSON.parse(returnData.Progress)[0];
                        var Products = JSON.parse(returnData.Products);
                        var Activity = JSON.parse(returnData.Activity);
                        $("#kt_charts_mixed_widget_16_chart").empty().attr('data-percent', Progress.CompanyProfile);
                        $("#InviteCustomer").attr('href', Progress.BaseURL);

                        //Products
                        $("#tHeadProduct").append('<tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0"><th class="min-w-200px">PRODUCT</th><th>CATEGORY</th><th>SELLING PRICE</th></tr>');
                        for (var i = 0; i < Products.length; i++) {
                            var full = Products[i];
                            $("#tBodyProduct").append('<tr><td><div class="d-flex align-items-center"><img src="' + full.PhotoIMG + '" class="w-40px me-3 rounded" alt="" />\
                                                           <div><span class="text-gray-800 text-hover-primary mb-1 table-name-trim" data-bs-toggle="tooltip" title="'+ full.Name + '">'+ full.Name + '</span>\
                                                           <div class="text-muted fs-8">'+ full.Code + '</div></div></div></td>\
                                                       <td><span href="#" class="text-gray-600 text-hover-primary mb-1">'+ full.ProductCategory + '</span></td>\
                                                       <td>'+ full.Price + '</td></tr >');
                        }

                        for (var i = 0; i < Activity.length; i++) {
                            var full = Activity[i];
                            $("#Activities").append('<div class="timeline-item"><div class="timeline-label fw-bolder text-gray-800 fs-7 w-90px">' + full.Date + '</div>\
                                                     <div class="timeline-badge"><i class="fa fa-genderless text-'+ GetColor(i) + ' fs-1"></i></div>\
                                                     <div class="timeline-content fw-bold text-gray-800 text-hover-primary ps-3" data-bs-toggle="tooltip" title="'+ full.Subject + '">'+ full.Description + '</div></div>');
                        }
                        $('[data-bs-toggle="tooltip"]').tooltip();
                        KTWidgets.init();
                    }
                    else toastr.error('Oops! Something went wrong. Please try again later.');
                },
                error: function () {toastr.error('Oops! Something went wrong. Please try again later.');}
            });
        }

        function showExtraStats(type) {
            $.ajax({
                url: "/Dashboard/GetExtraStats",
                type: "POST",
                data: { Type: type },
                success: function (returnData) {
                    if (returnData.Result == "OK") {

                        var data = JSON.parse(returnData.data);
                        //Products
                        if (type === 'Orders') {
                            $("#tHeadOrder").empty().append('<tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0"><th>Order No</th><th>Customers</th><th>Placed On</th><th>Status</th></tr>');
                            $("#tBodyOrder").empty();
                            for (var i = 0; i < data.length; i++) {
                                var full = data[i];
                                $("#tBodyOrder").append('<tr><td><a href="' + full.EnquiryURL + '" target="_blank" class="text-dark text-hover-primary fw-bold d-block fs-7">' + full.Code + '</a></td>\
                                                             <td><div class="d-flex align-items-center"><div class="d-flex justify-content-start flex-column">\
                                                                 <span class="text-dark fw-bolder fs-6 table-name-trim" data-bs-toggle="tooltip" title="'+ full.CustomerName + '">'+ full.CustomerName + '</a>\
                                                                 <span class="fw-bold text-muted d-block fs-7" data-bs-toggle="tooltip" title="'+ full.Email + '">'+ full.Email + '</span>\
                                                                 <span class=" text-dark d-block fs-8">'+ full.Mobile + '</span></div></div></td>\
                                                             <td><span class="text-muted fw-bold d-block fs-8">'+ full.Date + '</span><span class="text-dark fw-bolder d-block fs-7">'+ full.Amount + '</span></td>\
                                                             <td><span class="badge badge-light-'+ GetColrByOrderStatus(full.OrderStatusID) + ' fs-7 fw-bolder">' + full.OrderStatus + '</span></td></tr>');
                            }
                        }
                        else if (type === 'Customers') {
                            $("#tHeadCustomer").empty().append('<tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0"><th>Customers</th><th>Added On</th><th>Orders</th><th>Last Order</th></tr>');
                            $("#tBodyCustomer").empty();
                            for (var i = 0; i < data.length; i++) {
                                var full = data[i];
                                $("#tBodyCustomer").append('<tr><td><div class="d-flex align-items-center"><div class="d-flex justify-content-start flex-column">\
                                                                    <span class="text-dark fw-bolder text-hover-primary fs-6 table-name-trim" data-bs-toggle="tooltip" title="'+ full.Name + '">'+ full.Name + '</a>\
                                                                    <span class="fw-bold text-muted d-block fs-7" data-bs-toggle="tooltip" title="'+ full.Email + '">'+ full.Email + '</span>\
                                                                    <span class=" text-dark d-block fs-8">'+ full.Mobile + '</span></div></div></td>\
                                                                <td><span class="text-dark fw-bold d-block fs-7"> '+ full.CreatedDate + '</span><span class="text-muted fw-bolder d-block fs-8">' + full.CreatedTime+'</span></td>\
                                                                <td><span class="text-dark fw-bolder d-block fs-7">'+ full.Orders+'</span></td>\
                                                                <td><span class="text-dark fw-bold d-block fs-7">'+ full.LastOrderDate + '</span><span class="text-muted fw-bolder d-block fs-8">' + full.LastOrderTime +'</span></td></tr>');
                            }
                        }
                        else if (type === 'Queries') {
                            $("#Queries").empty();
                            for (var i = 0; i < data.length; i++) {
                                var full = data[i];
                                if (i > 0) $("#Queries").append('<div class="separator my-6"></div>');
                                $("#Queries").append('<div class="d-flex flex-wrap gap-2 flex-stack"><div class="d-flex align-items-center"><div class="symbol symbol-50 me-4">\
                                                <span class="symbol-label" style="background-image:url(/Content/assetsNew/media/avatars/blank.png);"></span></div>\
                                                <div class="pe-5"><div class="d-flex align-items-center flex-wrap gap-1"><a href="#" class="fw-bolder text-dark text-hover-primary">'+ full.Name + '</a>\
                                                <span class="svg-icon svg-icon-7 svg-icon-success mx-3"><svg xmlns="http://www.w3.org/2000/svg" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">\
                                                <circle fill="" cx="12" cy="12" r="8" /></svg></span>\
                                                <span class="text-muted fw-bolder">'+ full.Date + '</span></div>\
                                                <div class="text-muted fw-bold mw-450px" data-kt-inbox-message="preview">'+ full.Message + '</div></div></div>\
                                                <div class="d-flex align-items-center flex-wrap gap-2"><span class="fw-bold text-muted text-end me-3">'+ full.Email + '</span></div></div>');

                            }
                        }
                        else if (type === 'Catalogs') {
                            $("#tHeadCatalogs").empty().append('<tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0"><th>NAME</th><th>PRODUCTS</th><th>VIEWS</th></tr>');
                            $("#tBodyCatalogs").empty();
                            for (var i = 0; i < data.length; i++) {
                                var full = data[i];
                                $("#tBodyCatalogs").append('<tr><td><a href="' + full.CatalogURL +'" target="_blank" class="text-dark text-hover-primary fw-bolder d-block fs-7 table-name-trim">'+full.CatalogName+'</span></td>\
                                                                <td><div class="d-flex align-items-center flex-shrink-0 me-5 py-3"><div class="symbol-group symbol-hover flex-shrink-0">\
                                                                    <div class="symbol symbol-circle symbol-25px '+ (full.Photo1 === '' ? 'd-none' : '') + '"><img src="' + full.Photo1 + '" alt="" /></div>\
                                                                    <div class="symbol symbol-circle symbol-25px '+ (full.Photo2 === '' ? 'd-none' : '') + '"><img src="' + full.Photo2 + '" alt="" /></div>\
                                                                    <div class="symbol symbol-circle symbol-25px '+ (full.Photo3 === '' ? 'd-none' : '') + '"><img src="' + full.Photo3 + '" alt="" /></div>\
                                                                    <div class="symbol symbol-circle symbol-25px '+ (full.Photo4 === '' ? 'd-none' : '') + '"><img src="' + full.Photo4 + '" alt="" /></div></div>\
                                                                    <span class="btn btn-sm btn-light-success fw-bolder ms-1 fs-7 py-0 px-2">' + full.Products + '</span></div></td>\
                                                                <td><span class="text-dark-600 d-block fs-7">'+full.Visits+'</span></td></tr>');
                            }
                        }
                        $('[data-bs-toggle="tooltip"]').tooltip();
                    }
                    else toastr.error('Oops! Something went wrong. Please try again later.');
                },
                error: function () {toastr.error('Oops! Something went wrong. Please try again later.');}
            });
        }

        function skipOnboarding() {
            $.ajax({
                url: "/Dashboard/SkipOnboarding",
                type: "POST",
                data: { },
                success: function (returnData) {
                    if (returnData.Result == "OK") {
                        location.href = "/Home";
                    }
                    else toastr.error('Oops! Something went wrong. Please try again later.');
                },
                error: function () {toastr.error('Oops! Something went wrong. Please try again later.');}
            });
        }
    </script>    

