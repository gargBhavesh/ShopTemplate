        function uploadTodoToS3(blob, callback, elem) {
            KTApp.block('body', { type: 'v2', centerY: false, centerX: false, message: 'Processing..!! Please do not close the window until finish.' });
            var randomNo = 'xxxxxxxx'.replace(/[x]/g,
            function (c) {
                var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
            var file = new File([blob], (randomNo + '.webm'), { type: 'audio/webm' });
            var objKey = 'BIZ365/VoiceNotes/' + file.name;
            var params = {
                Key: objKey,
                ContentType: file.type,
                Body: file,
                ACL: 'bucket-owner-full-control'
            };

            s3.upload(params, function (err, s3Response) {
                KTApp.unblock('body');
                if (err) {
                    toastr.error("Error while  uploading audio");
                    callback(elem);
                } else {
                    callback(elem, s3Response.Location);
