            recordedChunks = [];
            const audioPlay = document.querySelector('#audioPlay');
            audioPlay.hidden = true;
            navigator.getUserMedia({ audio: true, video: false }, function (stream) {
                gStream = stream;
                mediaRecorder = new MediaRecorder(stream, { mimeType: 'audio/webm;' });
                mediaRecorder.onstop = handleStop;
                mediaRecorder.ondataavailable = handleDataAvailable;
                mediaRecorder.start();

                $("#RecordStop").show();
                $("#RecordMessage").hide();
                $("#RecordPlay").hide();
                $("#timerToStop").show();

                interval_Seconds = 0;
                interval_RecordingTime = setInterval(function () {
                    if (interval_Seconds === videoAllowedDuration) {
                        clearInterval(interval_RecordingTime);
                        if ($("#RecordStop").is(':visible'))
                            $("#RecordStop").click();
                    }
                    console.log(interval_Seconds);
                    $("#timerToStop").html(interval_Seconds.toString().toHHMMSS() + ' / ' + videoAllowedDuration.toString().toHHMMSS());
                    interval_Seconds++;
                }, 1000);

            }, function (err) { toastr.danger('No microphone found.') });
        }
