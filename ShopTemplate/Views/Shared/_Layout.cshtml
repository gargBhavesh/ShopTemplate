        }

        .no-auth .btn.btn-primary {
            background: #cf397e !important;
            color: #ffffff !important
        }

            .no-auth .btn.btn-primary:hover:not(.btn-active), .no-auth .btn.btn-primary:active, .no-auth .btn.btn-primary:focus {
                background-color: #af2864 !important;
            }


        .validation-summary-errors ul {
            list-style: none;
            padding: 0;
        }
        .modal_content {
            padding-bottom: 2rem;
        }

            .modal_content h4 {
                font-size: 1.125rem;
                line-height: 1.5rem;
                color: #000000;
                margin-bottom: 0.25rem;
            }

            .modal_content p {
                margin: 0.375rem 0;
            }

            .modal_content code {
                font-family: inherit;
                color: #42474c;
                background-color: #e8e9eb;
                margin: auto 2.5rem;
                padding: 0.25rem;
                line-height: 2.5rem;
            }

            .modal_content ul {
                margin: 0.625rem 2.5rem;
                list-style: circle;
            }

            .modal_content li, .modal_content p {
                font-size: 1rem;
                line-height: 1.5rem;
                color: #000000;
            }
        .gradient-bg {
            background-image: url('https://talsuite.s3.ap-south-1.amazonaws.com/BIZ365/Assets/splash.webp?1.0.2.4');
            background-repeat: no-repeat;
            background-position: 50%;
            background-size: cover;
            min-height: 100vh;
            height: fit-content;
        }
        .input__label {
            position: absolute;
            left: 0;
            top: 0;
            padding: 8px 10px;
            margin: 8px;
            white-space: nowrap;
            transform: translate(0, 0);
            transform-origin: 0 0;
            background: #fff;
            transition: transform 120ms ease-in;
            line-height: 1.2;
            font-size: 13px;
            color: #6f7780;
        }

        .input__field {
            box-sizing: border-box;
            display: block;
            width: 100%;
            border: 1px solid #e6e6e6;
            padding: 0px 16px;
            color: currentColor;
            background: #fff;
            border-radius: 4px;
            height:52px;
        }

            .input__field:not(:-moz-placeholder-shown) + .input__label {
                transform: translate(0.25rem, -65%) scale(0.8);
                color: #cf397e;
            }

            .input__field:not(:-ms-input-placeholder) + .input__label {
                transform: translate(0.25rem, -65%) scale(0.8);
                color: #cf397e;
            }

            .input__field:focus + .input__label, .input__field:not(:placeholder-shown) + .input__label {
                transform: translate(0.25rem, -65%) scale(0.8);
                    color: #cf397e;

            }

            .input__field:focus-visible {
                outline-offset: 0px;
                    border: 2px solid #cf397e;

                outline: none;
            }
            .input__field:-internal-autofill-selected {
                appearance: menulist-button;
            }
                .input__field:focus + .input__label, .input__field:not(:placeholder-shown) + .input__label {background-image: none !important;
                background-color: #ffffff !important;
            }
    </style>
    <style>
        .timeline-label:before {
            left: 91px;
        }

        .blob {
            background: black;
            box-shadow: 0 0 0 0 rgba(0, 0, 0, 1);
            transform: scale(1);
            animation: pulse-black 1.5s infinite;
            padding: 10px 20px;
            border-radius: 6px;
            color: #fff;
            font-weight: 500;
            font-size: 1.2em;
            cursor: pointer;
        }

            .blob.red {
                background-color: #50cd89;
                box-shadow: 0 0 0 0 #50cd89;
                animation: pulse-red 2s infinite;
            }

                .blob.red:hover {
                    color: #ffffff;
                    background-color: #47be7d !important;
                    animation: none;
                }

            .blob.btn-primary:hover {
                color: #ffffff !important;
            }

        @@keyframes pulse-red {
            0% {
                transform: scale(0.90);
                box-shadow: 0 0 0 0 #50cd89AA;
            }

            70% {
                transform: scale(1);
                box-shadow: 0 0 0 16px #50cd8900;
            }

            100% {
                transform: scale(0.90);
                box-shadow: 0 0 0 0 #50cd8900;
            }
        }
    </style>
