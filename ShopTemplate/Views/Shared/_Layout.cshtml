    <script>
        function displayToggle(id){
            if($(id).attr('style') == 'display : none') {
                $(id).attr('style','display : true')
            }
            else{
                $(id).attr('style','display : none')
            }
        }
        function showPassword(elem) {
            if ($(elem).prev().prev().attr('type') == 'password') {
                $(elem).prev().prev().attr('type', 'text');
                $(elem).find('i').removeClass('bi-eye-slash').addClass('bi-eye');
            }
            else {
                $(elem).prev().prev().attr('type', 'password');
                $(elem).find('i').removeClass('bi-eye').addClass('bi-eye-slash');
            }
        }
        var ApplicationType = 'Shopivana';
        var blockUI;
        function showLoader() {
            var target = document.querySelector('#kt_body1');
            blockUI = new KTBlockUI(target, { message: '<div class="blockui-message"><span class="spinner-border text-primary"></span> ' + (ApplicationType === 'Bananashop' ? 'Going Bananas...' : 'Processing...') + '</div>' });
            blockUI.block();
        }
        function hideLoader() {
            if (blockUI.isBlocked()) blockUI.release();
            blockUI.destroy();
        }
        function showInfo(Message) {
            Swal.fire({ text: Message, icon: "info", buttonsStyling: false, confirmButtonText: "Okay", customClass: { confirmButton: "btn btn-primary" } });
        }
        function showSuccess(Message, Url) {
            if (Url) Swal.fire({ html: Message, icon: "success", buttonsStyling: false, confirmButtonText: "Okay", customClass: { confirmButton: "btn btn-success" } }).then((result) => { if (result.isConfirmed) window.open(Url, "_self"); });
            else Swal.fire({ html: Message, icon: "success", buttonsStyling: false, confirmButtonText: "Okay", customClass: { confirmButton: "btn btn-success" } });
        }
        function showError(Message) {
            Swal.fire({ html: Message, icon: "error", buttonsStyling: false, confirmButtonText: "Okay", customClass: { confirmButton: "btn btn-danger" } });
        }
        function showWarning(Message) {
            Swal.fire({ html: Message, icon: "warning", buttonsStyling: false, confirmButtonText: "Okay", customClass: { confirmButton: "btn btn-warning" } });
        }
        function startProcess(selector) { document.querySelector(selector).setAttribute("data-kt-indicator", "on"); document.querySelector(selector).disabled = true; }
        function endProcess(selector) { document.querySelector(selector).removeAttribute("data-kt-indicator"); document.querySelector(selector).disabled = false; }
    
        localStorage.sentToServer = '0';
    
        function ValidateEmail(input) {
            var expr = /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (input.match(expr)) {
                return true;
            }
            else {
                return false;
            }
        }        
    </script>
