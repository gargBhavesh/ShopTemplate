}
.pb-10 {
    padding-bottom: 2.5rem!important;
}
.fv-plugins-icon-container {
    position: relative;
}
.btn.btn-icon .svg-icon, .btn.btn-icon i {
    padding: 0;
    margin: 0;
    line-height: 1;
}
.btn i {
    display: inline-flex;
    font-size: 1rem;
    padding-right: 0.35rem;
    vertical-align: middle;
    line-height: 0;
}
    </style>
    <style>
