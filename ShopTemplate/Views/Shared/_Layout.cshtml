    <style>
        .mid_size_font{
        
            font-size: 15px!important;
        }
        .bg-body {
    --bg-color: 255,255,255;
    background-color: #fff!important;
    padding : 0;
    
}body {
    display: flex;
    flex-direction: column;
    color: #181c32;
    
}

.content {
    padding: 30px 0;
}
.flex-xl-row {
    flex-direction: row!important;
}
.ms-lg-15 {
    margin-left: 3.75rem!important;
}
.w-xl-350px {
    width: 350px!important;
}

        .validation-summary-errors {
            font-size: 1rem;
            color: #f1416c;
        }
        .validation-summary-success {
            font-size: 1rem;
            color: #08cc6c;
        }

        .select2-container {
            padding: 0px;
        }

        .no-auth {
            display: flex;
            flex-direction: column;
            justify-content: center;
            min-height: 540px;
        }

        .no-auth .btn.btn-primary {
            background: #cf397e !important;
            color: #ffffff !important
        }

            .no-auth .btn.btn-primary:hover:not(.btn-active), .no-auth .btn.btn-primary:active, .no-auth .btn.btn-primary:focus {
                background-color: #af2864 !important;
            }


        .validation-summary-errors ul {
            list-style: none;
            padding: 0;
        }
        .modal_content {
            padding-bottom: 2rem;
        }

            .modal_content h4 {
                font-size: 1.125rem;
                line-height: 1.5rem;
                color: #000000;
                margin-bottom: 0.25rem;
            }

            .modal_content p {
                margin: 0.375rem 0;
            }

            .modal_content code {
                font-family: inherit;
                color: #42474c;
                background-color: #e8e9eb;
                margin: auto 2.5rem;
                padding: 0.25rem;
                line-height: 2.5rem;
            }

            .modal_content ul {
                margin: 0.625rem 2.5rem;
                list-style: circle;
            }

            .modal_content li, .modal_content p {
                font-size: 1rem;
                line-height: 1.5rem;
                color: #000000;
            }
        .gradient-bg {
            background-image: url('https://talsuite.s3.ap-south-1.amazonaws.com/BIZ365/Assets/splash.webp?1.0.2.4');
            background-repeat: no-repeat;
            background-position: 50%;
            background-size: cover;
            min-height: 100vh;
            height: fit-content;
        }
        .input__label {
            position: absolute;
            left: 0;
            top: 0;
            padding: 8px 10px;
            margin: 8px;
            white-space: nowrap;
            transform: translate(0, 0);
            transform-origin: 0 0;
            background: #fff;
            transition: transform 120ms ease-in;
            line-height: 1.2;
            font-size: 13px;
            color: #6f7780;
        }

        .input__field {
            box-sizing: border-box;
            display: block;
            width: 100%;
            border: 1px solid #e6e6e6;
            padding: 0px 16px;
            color: currentColor;
            background: #fff;
            border-radius: 4px;
            height:52px;
        }

            .input__field:not(:-moz-placeholder-shown) + .input__label {
                transform: translate(0.25rem, -65%) scale(0.8);
                color: #cf397e;
            }

            .input__field:not(:-ms-input-placeholder) + .input__label {
                transform: translate(0.25rem, -65%) scale(0.8);
                color: #cf397e;
            }

            .input__field:focus + .input__label, .input__field:not(:placeholder-shown) + .input__label {
                transform: translate(0.25rem, -65%) scale(0.8);
                    color: #cf397e;

            }

            .input__field:focus-visible {
                outline-offset: 0px;
                    border: 2px solid #cf397e;

                outline: none;
            }
            .input__field:-internal-autofill-selected {
                appearance: menulist-button;
            }
                .input__field:focus + .input__label, .input__field:not(:placeholder-shown) + .input__label {background-image: none !important;
                background-color: #ffffff !important;
            }
            .p10{
                padding: 2.5rem!important;
            }
            .fv-plugins-message-container.invalid-feedback{
                display: block;
                font-weight: 400
            }
            a {
    transition: color .2s ease,background-color .2s ease;
}
a {
    color: #009ef7;
    text-decoration: none;
}
            .flex-root {
